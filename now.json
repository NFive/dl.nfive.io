{
  "version": 2,
  "name": "nfive-redirect",
  "scope": "nfive",
  "alias": "dl.nfive.io",
  "regions": ["all"],
  "env": {
    "NODE_ENV": "production"
  },
  "builds": [
    {
      "src": "*.js",
      "use": "@now/node",
      "config": { "helpers": "false" }
    }
  ],
  "routes": [
    { "src": "/nfive.zip", "methods": ["GET"], "dest": "/nfive.js" },
    { "src": "/nfpm.exe", "methods": ["GET"], "dest": "/nfpm.js" },
    { "src": "/", "methods": ["GET"], "status": 301, "headers": { "Location": "https://nfive.io/" } },
    { "src": "/.+", "methods": ["GET"], "status": 404 }
  ]
}
